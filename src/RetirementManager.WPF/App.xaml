<Application x:Class="RetirementManager.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RetirementManager.WPF"
             xmlns:viewmodels="clr-namespace:RetirementManager.WPF.ViewModels"
             xmlns:views="clr-namespace:RetirementManager.WPF.Views">
    <Application.Resources>
        <DataTemplate DataType="{x:Type GroupBox}">
            <TextBlock>Hi</TextBlock>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:WorkerList}">
            <views:WorkerListView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:ClientList}">
            <views:ClientListView/>
        </DataTemplate>
    </Application.Resources>
</Application>
